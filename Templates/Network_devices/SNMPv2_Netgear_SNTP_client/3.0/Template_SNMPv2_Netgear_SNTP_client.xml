    <triggers>
        <trigger>
            <expression>{Template SNMPv2 Netgear SNTP client:agentSntpClientLastAttemptStatus.0.last(#1)}&lt;&gt;2 and&#13;
{Template SNMPv2 Netgear SNTP client:agentSntpClientLastAttemptStatus.0.last(#2)}&lt;&gt;2</expression>
            <name>SNTP attempt status on {HOST.NAME} was unsuccessfull ({ITEM.VALUE1})</name>
            <url/>
            <status>0</status>
            <priority>4</priority>
            <description>last and previous agentSntpClientLastAttemptStatus are not success(2) (ensure that the trigger only fires if at least tow values/attempts have failed)</description>
            <type>0</type>
            <dependencies>
                <dependency>
                    <name>SNTP client is disabled on {HOST.NAME}</name>
                    <expression>{Template SNMPv2 Netgear SNTP client:agentSntpClientMode.0.last()}=0</expression>
                </dependency>
            </dependencies>
        </trigger>
